<div class="container" id="main">
    <div class="row">
        <div class="col-md-6 col-sm-6">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <div class="col-md-8">
                        <h4>Drag your files below</h4>
                    </div>
                    <input type="button" value="Submit" class="btn btn-info" onclick="uploadFiles()" />
                </div>
                <div class="panel-body">
                    <div id="ulbox">
                        <span>Drop your files here...</span>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-6 col-sm-6">
            <div class="well">
                <div class="panel panel-default" id="pnlFile">
                    <div class="panel-heading"><h4>File Attributes</h4></div>
                    <div class="panel-body">
                        <div id="fileAttrSpan">
                            <span id="fileAttr">File Name:</span>
                        </div>
                        <table>
                            <thead>
                                <tr>
                                    <th>File Name</th>
                                    <th>Date Created</th>
                                </tr>
                            </thead>
                            <tbody data-bind="foreach: Ufiles">
                                <tr>
                                    <td><span data-bind="text: name" /></td>
                                    <td><span data-bind="text: created" /></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div><!--/row-->
    <div id="divTest">
        <p>First name: <strong data-bind="text:  ko.toJSON($data)">todo</strong></p>
        <p>Last name: <strong data-bind="text: lastName">todo</strong></p>
    </div>

    <div class="clearfix"></div>

    <hr>

</div><!--/main-->
